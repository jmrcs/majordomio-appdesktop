# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'majordomo' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for majordomo
  pod 'RxSwift',    '~> 2.0'
  pod 'RxCocoa',    '~> 2.0'

  target 'majordomoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 2.0'
    pod 'RxTests',    '~> 2.0'
  end

  target 'majordomoUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 2.0'
    pod 'RxTests',    '~> 2.0'
  end

end
